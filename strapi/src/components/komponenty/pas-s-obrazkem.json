{
  "collectionName": "components_komponenty_pas_s_obrazkems",
  "info": {
    "displayName": "Pás s obrázkem",
    "icon": "layout",
    "description": ""
  },
  "options": {},
  "attributes": {
    "Nadpis": {
      "type": "string"
    },
    "Fotka": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Zarovnani": {
      "type": "enumeration",
      "default": "Nahoru",
      "enum": [
        "Nahoru",
        "Na střed"
      ]
    },
    "Umisteni_fotky": {
      "type": "enumeration",
      "default": "vlevo",
      "enum": [
        "vlevo",
        "vpravo"
      ]
    },
    "Text": {
      "type": "richtext"
    },
    "Tlacitko": {
      "type": "component",
      "component": "elementy.tlacitko",
      "repeatable": true
    },
    "Pozadi": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Pozadi_barva": {
      "type": "customField",
      "customField": "plugin::color-picker.color",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$"
    }
  }
}
